import turtle as t

t.shape("circle")
t.resizemode("user")
t.pensize(5)
t.speed(10)
t.color('red', 'blue')
t.title("Turtle Try")
t.begin_fill()
while True:
    t.forward(360)
    t.left(148)
    if abs(t.pos()) < 1:
        break
t.end_fill()
t.left(90)
t.forward(360)
t.left(120)
t.forward(480)
t.left(120)
t.forward(480)
t.left(120)
t.forward(480)
t.filling()
t.done()
