public class Ex11 {
    
    public static void main(String[] args) {
        Bike bike = new Bike();
        Bus bus = new Bus();
        bike.run();
        bus.run();
    }    
}

abstract class Car {
    abstract void run();
}

class Bike extends Car {
    void run() {
        System.out.println("这是一辆自行车");
    }
}

class Bus extends Car {
    void run() {
        System.out.println("这是一辆巴士");
    }
}