import java.util.Random;

public class Test {
    public static void main(String[] args) {
        
        int a[] = new int[12];
        int x = -65;
        int y = 65;
        Random r = new Random();

        System.out.printf("a[] = ");
        for (int i = 0; i < a.length; i++){
            
            a[i] = r.nextInt(128)-64;
            System.out.printf(a[i] + " ");
        }

        for (int i = 0; i < a.length; i++){

            int z = a[i];

            if (x < z) {
                x = z;
            }
            if (y > z) {
                y = z;
            }   
        }
        System.out.printf("\n最大数为:" + x + "\n");
        System.out.printf("最小数为:" + y + "\n");
    }
}